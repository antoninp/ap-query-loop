{
  "apiVersion": 3,
  "name": "apql/gallery",
  "title": "APQL Gallery",
  "description": "Display a list of posts from a selected post type as a gallery using featured images.",
  "category": "theme",
  "icon": "images-alt2",
  "textdomain": "apql-gallery",
  "ancestor": ["core/query"],
  "usesContext": ["query", "queryId", "apql/filterTax", "apql/filterTerm"],
  "supports": { "html": false },
  "attributes": {
    "layout": { "type": "string" },
    "columns": { "type": "number" },
    "imageSize": { "type": "string" },
    "gutter": { "type": "number" },
    "rowHeight": { "type": "number" },
    "animation": { "type": "string" },
    "captions": { "type": "string" },
    "link": { "type": "string" },
    "customClass": { "type": "string" },
    "align": { "type": "string" },
    "size": { "type": "string" }
  },
  "editorScript": "file:./build/index.js",
  "style": "file:./build/style-index.css"
}
