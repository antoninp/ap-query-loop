{
  "apiVersion": 3,
  "name": "ap/group-by-tax",
  "title": "AP Group by Taxonomy",
  "description": "Group the current Query Loop posts by a taxonomy and render content per term. Use InnerBlocks to compose your layout.",
  "category": "theme",
  "icon": "filter",
  "textdomain": "ap-query-loop",
  "parent": ["core/query"],
  "usesContext": ["query", "queryId"],
  "providesContext": {
    "ap/groupTax": "taxonomy",
    "ap/groupTerm": "currentTermSlug",
    "ap/currentTerm": "currentTerm"
  },
  "supports": { "html": false },
  "attributes": {
    "taxonomy": { "type": "string", "default": "aplb_library_pdate" },
    "currentTermSlug": { "type": "string" },
    "currentTerm": { "type": "object" }
  }
}
